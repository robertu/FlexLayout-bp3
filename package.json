{
  "name": "flexlayout-react-bp3",
  "version": "0.3.4",
  "description": "A multi-tab layout manager with blueprintjs",
  "main": "lib/index.js",
  "types": "./declarations/index.d.ts",
  "keywords": [
    "react",
    "layout",
    "dock",
    "panel",
    "tabs",
    "drag-and-drop",
    "tabset",
    "splitter",
    "drag",
    "drop",
    "reactjs",
    "react-dom"
  ],
  "scripts": {
    "devserver": "webpack-dev-server",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "tsc -p tsconfig2.json",
    "build:umd": "webpack --config webpack_build.config.js",
    "build:umd:min": "webpack -p --config webpack_build_min.config.js",
    "test": "jasmine",
    "lint": "tslint -c tslint.json 'src/**/*.ts*' ",
    "doc": "typedoc --out typedoc --mode file --exclude \"**/examples/**/*.tsx\"  --readme none "
  },
  "author": "Caplin Systems Ltd and Robert Urba≈Ñczyk",
  "repository": "https://github.com/caplin/FlexLayout",
  "license": "ISC",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.16",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "awesome-typescript-loader": "^5.2.0",
    "jasmine": "^3.3.12",
    "prettier": "^1.17.1",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.19.1"
  }
}

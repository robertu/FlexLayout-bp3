{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rules": {
      "interface-name": false,
      "semicolon": [
        true,
        "always"
      ],
      "quotemark": [
        true,
        "double",
        "jsx-double"
      ],
      "member-access": [true, "no-public"],
      "no-empty": [true, "allow-empty-catch"],
      "jsx-no-lambda": false,
      "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-leading-underscore"
      ],
      "object-literal-sort-keys": false
    }
  }